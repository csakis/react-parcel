{
  "manifest_version": 2,
  "name": "Chrome Extension ip adress parser ",
  "short_name": "ipParser",
  "version": "1.0.0",
  "description": "A React Chrome Extension using parcel bundling ",
  "icons": {
    "16": "src/static/icons/16.png",
    "48": "src/static/icons/48.png",
    "128": "src/static/icons/128.png"
  },
  "browser_action": {
    "default_title": "React Tic Tac Toe",
    "default_popup": "src/static/index.html"
  },
  "permissions": [],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["src/content.js"]
    }
  ]
}
