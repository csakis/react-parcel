{
  "manifest_version": 2,
  "name": "Chrome Extension Tic Tac Toe",
  "short_name": "TicTacToe",
  "version": "1.0.0",
  "description": "A React Chrome Extension using Redux and parcel bundling ",
  "icons": {
    "16": "src/static/icons/16.png",
    "48": "src/static/icons/48.png",
    "128": "src/static/icons/128.png"
  },
  "browser_action": {
    "default_title": "React Tic Tac Toe",
    "default_popup": "src/static/index.html"
  },
  "permissions": []
}